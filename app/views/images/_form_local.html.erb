<%= form_with(model: image, local: true, multipart: true) do |form| %>
  <div class="form-group">
    <%= form.file_field :data, id: :image_data, class: 'file-field-style' %>
  </div>
  <div class="form-group">
    <%= form.submit "Proceed To Next Step", class: 'btn btn-success btn-lg link-button' %>
  </div>
<% end %>